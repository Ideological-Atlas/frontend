{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.28.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "audit": "pnpm audit",
    "prepare": "test -d .git && lefthook install || echo Skipping",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --check .",
    "validate": "pnpm run format && pnpm run format:check && pnpm run lint:fix && pnpm run typecheck && pnpm audit",
    "regenerateClient:local": "curl -s -H 'Accept-Language: en' http://localhost:3141/api/schema/ > swagger-temp.yaml && openapi -i swagger-temp.yaml -o src/lib/client && cp src/lib/custom-request.ts src/lib/client/core/request.ts && rm swagger-temp.yaml && pnpm validate && pnpm format"
  },
  "dependencies": {
    "@hookform/resolvers": "5.2.2",
    "@radix-ui/react-switch": "1.2.6",
    "@react-oauth/google": "0.13.4",
    "@t3-oss/env-nextjs": "0.13.10",
    "canvas-confetti": "1.9.4",
    "clsx": "2.1.1",
    "framer-motion": "12.25.0",
    "js-cookie": "^3.0.5",
    "material-symbols": "0.40.2",
    "next": "16.1.1",
    "next-intl": "4.7.0",
    "next-themes": "0.4.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "7.71.0",
    "tailwind-merge": "3.4.0",
    "tailwind-variants": "3.2.2",
    "zod": "4.3.5",
    "zustand": "5.0.9"
  },
  "devDependencies": {
    "@commitlint/cli": "20.3.1",
    "@commitlint/config-conventional": "20.3.1",
    "@eslint/compat": "2.0.1",
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@evilmartians/lefthook": "2.0.13",
    "@next/eslint-plugin-next": "16.1.1",
    "@opennextjs/cloudflare": "1.15.1",
    "@tailwindcss/postcss": "4.1.18",
    "@types/canvas-confetti": "1.9.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "20.19.27",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.1",
    "eslint-plugin-next": "0.0.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "globals": "17.0.0",
    "lint-staged": "16.2.7",
    "openapi-typescript-codegen": "0.30.0",
    "prettier": "3.7.4",
    "prettier-plugin-tailwindcss": "0.7.2",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "typescript-eslint": "8.52.0",
    "vercel": "50.5.0",
    "wrangler": "4.60.0"
  },
  "pnpm": {
    "overrides": {
      "@smithy/config-resolver": "4.4.6",
      "path-to-regexp": ">=6.3.0",
      "tar": ">=7.5.4",
      "undici": ">=6.23.0"
    }
  }
}
